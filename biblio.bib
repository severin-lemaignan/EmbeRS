@Article{oshea2007MayJun.,
  author       = {O'Shea, Simone D. and Taylor, Nicholas F. and Paratz, Jennifer D.},
  journaltitle = {Journal of Cardiopulmonary Rehabilitation and Prevention},
  title        = {. . . {{But}} Watch out for the Weather: Factors Affecting Adherence to Progressive Resistance Exercise for Persons with {{COPD}}},
