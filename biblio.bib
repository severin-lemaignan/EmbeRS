  date         = {2017-08},
  journaltitle = {Personal and Ubiquitous Computing},
  title        = {A Word of Advice: How to Tailor Motivational Text Messages Based on Behavior Change Theory to Personality and Gender},
  doi          = {10.1007/s00779-017-1025-1},
